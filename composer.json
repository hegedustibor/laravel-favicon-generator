{
    "name": "spatie/Laravel Favicon Generator",
    "description": "",
    "keywords": [
        "spatie",
        "Laravel Favicon Generator"
    ],
    "homepage": "https://github.com/spatie/Laravel Favicon Generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Tibor Heged√ºs",
            "email": "contact@hegedustibor.hu",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Hegedustibor\\LaravelFaviconGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hegedustibor\\LaravelFaviconGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hegedustibor\\LaravelFaviconGenerator\\LaravelFaviconGeneratorServiceProvider"
            ],
            "aliases": {
                "LaravelFaviconGenerator": "Hegedustibor\\LaravelFaviconGenerator\\LaravelFaviconGeneratorFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
